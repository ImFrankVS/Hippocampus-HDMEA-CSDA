    "# ----------------------------------------------------------------------------------------- #\n",
    "# Active Channel Detection\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Custom made functions\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Path to the folder where the custom functions are storage\n",
    "PATHMODULE = joinpath( homedir( ), \""path-example"\", \"Code\" ); #                           *\n",
    "push!( LOAD_PATH, PATHMODULE );\n",
    "using ACD\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Julia's native packages\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "using JLD2\n",
    "using StatsBase\n",
    "using Plots\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Searching for directories and files to use\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "PATHSTART = joinpath( homedir( ), \""path-example-exmaple"\" ); #                                            *\n",
    "PATHSINFO, _ = FindDirsFiles( PATHSTART, \"Info\" );\n",
    "for i in 1:length( PATHSINFO )\n",
    "    f = PATHSINFO[ i ];\n",
    "    println( string( \"$i - $f\" ) );\n",
    "end\n",
    "Infos = length( PATHSINFO );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Select a file to work with from now on.\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "info = 0;\n",
    "while info > Infos || info < 1\n",
    "    print( \"Enter the file number: [ between 1 and $Infos ]\" );\n",
    "    info = parse( Int, readline( ) );\n",
    "end\n",
    "PATHINFO = PATHSINFO[ info ];\n",
    "println( \"\\nPath selected: $PATHINFO\" ); \n",
    "# ----------------------------------------------------------------------------------------- #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6bfb53cd",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Generating the new folders and files\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "PATHMAIN = dirname( PATHINFO );\n",
    "PATHFIGURES = joinpath( PATHMAIN, \"Figures\" );\n",
    "PATHFIGURES_GENERAL = joinpath( PATHFIGURES, \"GENERAL\" );\n",
    "#\n",
    "FILESTEP01 = joinpath( PATHMAIN, \"Info\", \"STEP01.jld2\" );\n",
    "FILEVARIABLES = joinpath( PATHINFO, \"Variables.jld2\" );\n",
    "FILEACD = joinpath( PATHINFO, \"ACD.jld2\" );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Extracting the variables contained in the brw file\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "Variables = LoadDict( FILEVARIABLES );\n",
    "step01 = LoadDict( FILESTEP01 );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# Constants\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "#= \n",
    "    All of the following values fall into the * category, \n",
    "    meaning that they can or should be modified\n",
    "=#\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "lower_limit = 2000;\n",
    "min_neigh = 6/8;\n",
    "min_patches = 3;\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "Cardinality = step01[ \"Cardinality\" ];\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "Empties = step01[ \"Empties\" ]; # List of previously discarded channels\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "W = sum( Cardinality, dims = 1 )[ 1 ];\n",
    "W = PatchEmpties( W, Empties );\n",
    "W = zscore( W );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "ACD_all, PACD = GetSupThr( W, lower_limit );\n",
    "FILEFIGURE = joinpath( PATHFIGURES_GENERAL,\"ACD_v0\" );\n",
    "Plots.png( PACD, FILEFIGURE );\n",
    "Plots.svg( PACD, FILEFIGURE );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "_, G = GetGroups( ACD_all );\n",
    "acd, pacd = GetPatches( G, min_patches );\n",
    "FILEFIGURE = joinpath( PATHFIGURES_GENERAL,\"ACD_v1\" );\n",
    "Plots.png( pacd, FILEFIGURE );\n",
    "Plots.svg( pacd, FILEFIGURE );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "newchs = copy( acd );\n",
    "for ch in acd\n",
    "    _, neigh = Neighbours( ch, 1 );\n",
    "    if length( neigh[ neigh .âˆˆ [ newchs ] ] ) / length( neigh ) >= min_neigh\n",
    "        newchs = unique( vcat( newchs, neigh ) );\n",
    "    end\n",
    "end\n",
    "_, G = GetGroups( newchs );\n",
    "acd, pacd = GetPatches( G, min_patches );\n",
    "FILEFIGURE = joinpath( PATHFIGURES_GENERAL,\"ACD_v2\" );\n",
    "Plots.png( pacd, FILEFIGURE );\n",
    "Plots.svg( pacd, FILEFIGURE );\n",
    "jldsave( FILEACD; Data = newchs );\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "\n",
    "# ----------------------------------------------------------------------------------------- #\n",
    "# end of ACD process\n",
    "# ----------------------------------------------------------------------------------------- #"
   ]
  }
